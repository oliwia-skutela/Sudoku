<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_create"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.oliwia.sudoku.Create">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Chronometer
                android:id="@+id/chronometer3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />



        </TableRow>


        <TableRow>

            <TableLayout
                android:id="@+id/tableLayoutSudoku2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></TableLayout>


        </TableRow>

        <TableRow android:id="@+id/buttonRow2">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow>

                    <Button
                        android:id="@+id/one"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="1" />

                    <Button
                        android:id="@+id/two"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="2" />

                    <Button
                        android:id="@+id/three"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="3" />

                    <Button
                        android:id="@+id/four"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="4" />

                    <Button
                        android:id="@+id/five"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="5" />

                    <Button
                        android:id="@+id/six"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="6" />
                </TableRow>

                <TableRow>

                    <Button
                        android:id="@+id/seven"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="7" />

                    <Button
                        android:id="@+id/eight"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="8" />

                    <Button
                        android:id="@+id/nine"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="9" />

                    <Button
                        android:id="@+id/delete2"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="usuń" />

                    <Button
                        android:id="@+id/check2"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:text="Sprawdź" />
                </TableRow>

            </TableLayout>

        </TableRow>
    </TableLayout>


</LinearLayout>